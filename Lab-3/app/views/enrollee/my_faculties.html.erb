<nav class="navbar navbar-default ">
  <div class="container-fluid">
    <div class="navbar-header">
      <div class="navbar-brand"><%= t("common.university") %></div>
    </div>
    <ul class="nav navbar-nav">
      <li><%= link_to t("common.home") , enrollee_index_path %></li>
      <li><%= link_to t("common.sign_out") , destroy_user_session_path , method: :delete  %></li>
    </ul>
  </div>
</nav>
<ul class="list-group">
  <li class="list-group-item">
    <h3 class="text-center"> <%= t(".enrolled_list") %> </h3>
  </li>
</ul>
<% if @faculties %>
    <% @faculties.each do |faculty| %>
        <ul class="list-group">
          <li class="list-group-item active"><%=faculty.name%></li>
          <li class="list-group-item "><%= t(".limit") %> <%=faculty.limit%></li>
          <li class="list-group-item"><%= link_to t(".check_register"), enrollee_admission_page_path(:faculty_id => faculty.id) %></li>
        </ul>
    <%end %>
<% else %>
    <ul class="list-group">
      <li class="list-group-item"><%= t(".no_faculties") %>  </li>
    </ul>
<% end %>
